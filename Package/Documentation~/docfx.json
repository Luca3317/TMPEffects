"metadata": [
    {
      "src": [
        {
          "src": "../../..",
          "files": [
            "TMPEffects.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "videos/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "TMPEffects",
      "_appTitle": "TMPEffects",
      "_enableSearch": true,
      "pdf": true
    }
  }
}